// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node-modules":true,
        "**/bower_components":true,
        "**/*.js": { "when": "$(basename).js" },
        "**/*.js.map": { "when": "$(basename).js.map" }
    }
}